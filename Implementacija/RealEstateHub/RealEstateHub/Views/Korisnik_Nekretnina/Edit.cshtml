@model RealEstateHub.Models.Korisnik_Nekretnina

@{
    ViewData["Title"] = "Edit";
}

<h1>Uredi</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="kn_id" />
            <div class="form-group">
                <label asp-for="nekretninaId" class="control-label"></label>
                <input asp-for="nekretninaId" class="form-control" />
                <span asp-validation-for="nekretninaId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="korisnikId" class="control-label"></label>
                <input asp-for="korisnikId" class="form-control" />
                <span asp-validation-for="korisnikId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Sacuvaj" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Vrati se na listu</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
